{
  "name": "blog",
  "version": "1.0.3",
  "description": "A boilerplate for building a fast static website with the Eleventy static site generator, for deployment to Netlify.",
  "scripts": {
    "serve": "DEBUG=Eleventy* npx @11ty/eleventy --serve",
    "build": "npx @11ty/eleventy",
    "watch": "npx @11ty/eleventy --watch",
    "debug": "DEBUG=* npx @11ty/eleventy",
    "start": "http-server _site -p 8081"
  },
  "engines": {
    "node": "^20"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/enjikaka/blog"
  },
  "author": {
    "name": "Jeremy Karlsson",
    "email": "karlsson@jeremy.se",
    "url": "https://jeremy.se"
  },
  "contributors": [
    {
      "name": "Dan Urbanowicz",
      "email": "dan.urbanowicz@gmail.com",
      "url": "https://www.danurbanowicz.com/"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/danurbanowicz/eleventy-netlify-boilerplate/issues"
  },
  "homepage": "https://github.com/danurbanowicz/eleventy-netlify-boilerplate",
  "devDependencies": {
    "@11ty/eleventy": "^3.0.0",
    "@11ty/eleventy-fetch": "^5.0.2",
    "@11ty/eleventy-img": "^6.0.1",
    "@11ty/eleventy-navigation": "^0.3.5",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
    "clean-css": "^5.3.3",
    "html-minifier": "^4.0.0",
    "http-server": "^14.1.1",
    "luxon": "^3.5.0",
    "markdown-it": "^14.1.0",
    "markdown-it-anchor": "^9.2.0",
    "uglify-js": "^3.19.3"
  }
}
